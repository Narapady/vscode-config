// Place your key bindings in this file to override the defaults
[
  {
    "key": "cmd+g",
    "command": "GitLens: Open Changes with Next Revision"
  },
  {
    "key": "cmd+k",
    "command": "projectManager.listGitProjects#sideBarGit"
  },
  {
